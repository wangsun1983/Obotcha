ioDir = io

objs += $(outtmp)/file.o \
	$(outtmp)/fileinputstream.o \
	$(outtmp)/fileoutputstream.o \
	$(outtmp)/printstream.o \
	$(outtmp)/zipfilestream.o \
	$(outtmp)/zipmemorystream.o \
	$(outtmp)/filewatcher.o \

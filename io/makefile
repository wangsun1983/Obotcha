$(outtmp)/filenotfoundexception.o :$(ioDir)/FileNotFoundException.cpp
	$(cppcompiler) $(obotchacppflags) -c $(ioDir)/FileNotFoundException.cpp -o $(outtmp)/filenotfoundexception.o $(external)

